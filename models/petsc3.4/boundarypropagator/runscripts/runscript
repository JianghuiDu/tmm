mpiexec -np 2 ./tmm -numtracers 3 \
  -i Gini_01.petsc,Gini_02.petsc,Gini_03.petsc \
  -me Ae1.petsc -mi Ai1.petsc -mbe Be.petsc -mbi Bi.petsc \
  -t0 0.0 -iter0 0 -dt 0.0013888888888889 \
  -o G_01.petsc,G_02.petsc,G_03.petsc \
  -max_steps 7200 -write_steps 720 -prescribed_bc \
  -bc_files Gbc_01.petsc,Gbc_02.petsc,Gbc_03.petsc \
  -bc_cutoff_step 720 > log0
